{
  "compilerOptions": {
    "incremental": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "ES2022",
    "esModuleInterop": true,
    "removeComments": true,
    "importHelpers": true,
  "types": ["node", "jest"],
    "skipLibCheck": true
  },
  "compileOnSave": true,
  "exclude": ["node_modules", "lib"]
}
